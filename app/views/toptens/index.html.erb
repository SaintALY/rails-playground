<p id="notice"><%= notice %></p>

<h1>Toptens</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Comment</th>
      <th>Address</th>
      <th>Stars</th>
      <th colspan="3"></th>
    </tr>
  </thead>
 
  <tbody>
    <% @toptens.each do |topten| %>
      <tr>
        <td><%= topten.name %></td>
        <td><%= topten.comment %></td>
        <td><%= topten.address %></td>
        <td><%= topten.stars %></td>
        <td><%= link_to 'Show', topten %></td>
        <td><%= link_to 'Edit', edit_topten_path(topten) %></td>
        <td><%= link_to 'Destroy', topten, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

